.root div.main-menu {
    margin: auto;

    flex-direction: column;
    align-items: center;
    align-content: center;

    background-color: white;
}

.root div.main-background {
    position-type: absolute;
    top: 0px;
    left: 0px;

    width: 100%;
    height: 100%;

    z-index: -1l;

}

.root div.main-background img {
    height: 100%;
    width: 100%;
}
